# Root .gitignore for the E-CommerceWebsite repository

# Maven / build outputs
/target/
/**/target/
/build/
/out/
/dist/

# Maven POM backup and release files
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
dependency-reduced-pom.xml
release.properties

# Generated sources and reports
/generated-sources/
/generated-test-sources/
/surefire-reports/
/failsafe-reports/
jacoco.exec

# Packaging
*.jar
*.war
*.ear

# IDEs and editors
/.idea/
*.iml
*.iws
*.ipr
.vscode/
.settings/

# Eclipse
.classpath
.project

# NetBeans
/nbproject/private/
/nbbuild/
/nbdist/
.nb-gradle/

# OS files
.DS_Store
Thumbs.db

# Logs and runtime
*.log
hs_err_pid*

# Misc
*.class
*.swp
.env
*.local
local.properties

# Don't ignore the Maven wrapper jar - it's tracked by default
# .mvn/wrapper/maven-wrapper.jar

# Allow exceptions (examples)
# !/important-file.txt

